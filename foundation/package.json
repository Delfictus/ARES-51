{
  "name": "@novacore/csf-wasm",
  "version": "0.1.0",
  "description": "NovaCore ARES CSF WebAssembly bindings",
  "main": "index.js",
  "types": "index.d.ts",
  "scripts": {
    "build": "wasm-pack build --target web crates/csf-ffi",
    "build:node": "wasm-pack build --target nodejs crates/csf-ffi",
    "build:bundler": "wasm-pack build --target bundler crates/csf-ffi",
    "test": "wasm-pack test --headless --firefox --chrome",
    "lint": "eslint .",
    "format": "prettier --write .",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "novacore",
    "wasm",
    "webassembly",
    "real-time",
    "computing",
    "rust"
  ],
  "author": "NovaCore Team",
  "license": "MIT OR Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/Delfictus/NovaCore.git"
  },
  "bugs": {
    "url": "https://github.com/Delfictus/NovaCore/issues"
  },
  "homepage": "https://github.com/Delfictus/NovaCore#readme",
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "@wasm-tool/wasm-pack-plugin": "^1.7.0",
    "eslint": "^8.55.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.0.1",
    "prettier": "^3.1.0",
    "typescript": "^5.3.0",
    "wasm-pack": "^0.0.0",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.2.2"
  },
  "dependencies": {
    "@msgpack/msgpack": "^3.0.0-beta2",
    "dotenv": "^17.2.1"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  }
}
